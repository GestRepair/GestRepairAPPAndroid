<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.obscu.gestrepair5.Schedule_Service"
    tools:showIn="@layout/activity_schedule__service">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data"
        android:layout_below="@+id/spn_Service"
        android:layout_alignLeft="@+id/spn_Service"
        android:layout_alignStart="@+id/spn_Service"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/spn_Service" />

    <TextView
        android:id="@+id/txt_Vehicle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VeÃ­culo"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/txt_Service"
        android:layout_alignStart="@+id/txt_Service"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/txt_Service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Service"
        android:layout_below="@+id/spn_Vehicle"
        android:layout_alignLeft="@+id/spn_Vehicle"
        android:layout_alignStart="@+id/spn_Vehicle"
