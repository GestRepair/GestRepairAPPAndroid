<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ipt.gestrepair">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!-- To auto-complete the email text field in the login form with the user's emails
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="MainActivity2"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="Login"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Service"
            android:parentActivityName="ListServices"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_services">
        </activity>
        <activity android:name="Configurations"
            android:screenOrientation="portrait"/>
        <activity android:name="ListServices"
            android:parentActivityName="MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_list_services"/>
        <activity
            android:screenOrientation="portrait"
            android:name="ListVehicles"
            android:parentActivityName="MainActivity"
            android:label="@string/title_activity_list_vehicles"/>
        <activity
            android:screenOrientation="portrait"
            android:name="Schedule_Service"
            android:parentActivityName="MainActivity"
            android:label="@string/title_activity_schedule__service" />
        <activity
            android:screenOrientation="portrait"
            android:name="ListScheduleService"
            android:parentActivityName="MainActivity"
            android:label="@string/title_activity_list_schedule_service" />
        <activity
            android:screenOrientation="portrait"
            android:name="ListBudgets"
            android:parentActivityName="MainActivity"
            android:label="@string/title_activity_list_budgets" />
        <activity
            android:screenOrientation="portrait"
            android:name="Budgets"
            android:parentActivityName="ListBudgets"
            android:label="@string/title_activity_budgets"/>
        <activity
            android:screenOrientation="portrait"
            android:name="Budgets2"
            android:parentActivityName="ListBudgets"
            android:label="@string/title_activity_budgets"/>
        <activity
            android:screenOrientation="portrait"
            android:name="Vehicle"
            android:parentActivityName="ListBudgets"
            android:label="@string/title_activity_vehicle" />
        <activity
            android:screenOrientation="portrait"
            android:name="ListRepair"
            android:parentActivityName="MainActivity"
            android:label="@string/title_activity_list_repair" />
        <activity
            android:screenOrientation="portrait"
            android:name="Repair"
            android:parentActivityName="ListRepair"
            android:label="@string/title_activity_repair" />
        <activity
            android:screenOrientation="portrait"
            android:parentActivityName="ListScheduleService"
            android:name="SetScheduleService"
            android:label="@string/title_activity_set_schedule_service"></activity>
    </application>

</manifest>